{
  "name": "wamaxedu-platform",
  "version": "1.0.0",
  "description": "WAmaxEdu Platform - WhatsApp Education Platform",
  "main": "./dist/main.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc --noEmit",
    "start": "tsx ./src/main.ts",
    "start:prod": "node dist/main",
    "start:integrations": "tsx ./src/modules/integrations/src/main.ts",
    "start:messages": "tsx ./src/modules/messages/src/main.ts",
    "start:auth": "tsx ./src/modules/auth/src/main.ts",
    "start:realtime": "tsx ./src/modules/realtime/src/main.ts",
    "dev:server": "tsx watch ./src/main.ts",
    "test": "set NODE_ENV=test&& set REDIS_ENABLED=0&& set IDEMPOTENCY_STORE=memory&& set RABBITMQ_ENABLED=0&& tsx src/modules/integrations/test/integration.test.ts",
    "lint": "eslint --fix --ext .ts src",
    "lint:check": "eslint --ext .ts src",
    "commit": "cz",
    "commitlint": "commitlint --edit",
    "db:generate": "node runWithProvider.js \"npx prisma generate --schema ./prisma/DATABASE_PROVIDER-schema.prisma\"",
    "db:deploy": "node runWithProvider.js \"rm -rf ./prisma/migrations && cp -r ./prisma/DATABASE_PROVIDER-migrations ./prisma/migrations && npx prisma migrate deploy --schema ./prisma/DATABASE_PROVIDER-schema.prisma\"",
    "db:deploy:win": "node runWithProvider.js \"xcopy /E /I prisma\\DATABASE_PROVIDER-migrations prisma\\migrations && npx prisma migrate deploy --schema prisma\\DATABASE_PROVIDER-schema.prisma\"",
    "db:studio": "node runWithProvider.js \"node runWithProvider.js \"npx prisma studio --schema ./prisma/DATABASE_PROVIDER-schema.prisma\"",
    "db:migrate:dev": "node runWithProvider.js \"rm -rf ./prisma/migrations && cp -r ./prisma/DATABASE_PROVIDER-migrations ./prisma/migrations && npx prisma migrate dev --schema ./prisma/DATABASE_PROVIDER-schema.prisma && cp -r ./prisma/migrations/* ./prisma/DATABASE_PROVIDER-migrations\"",
    "db:migrate:dev:win": "node runWithProvider.js \"xcopy /E /I prisma\\DATABASE_PROVIDER-migrations prisma\\migrations && npx prisma migrate dev --schema prisma\\DATABASE_PROVIDER-schema.prisma\"",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wamaxedu/platform.git"
  },
  "keywords": [
    "whatsapp",
    "education",
    "platform",
    "monorepo",
    "typescript"
  ],
  "author": {
    "name": "WAmaxEdu Team"
  },
  "license": "Apache-2.0",
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "eslint --fix"
    ],
    "src/**/*.ts": [
      "sh -c 'tsc --noEmit'"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@adiwajshing/keyed-db": "^0.2.4",
    "@aws-sdk/client-sqs": "^3.891.0",
    "@hapi/boom": "^10.0.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^6.16.2",
    "@sentry/node": "^10.12.0",
    "@types/uuid": "^10.0.0",
    "amqplib": "^0.10.5",
    "axios": "^1.7.9",
    "baileys": "7.0.0-rc.9",
    "class-validator": "^0.14.1",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "zod": "^3.23.8",
    "eventemitter2": "^6.4.9",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "form-data": "^4.0.1",
    "https-proxy-agent": "^7.0.6",
    "i18next": "^23.7.19",
    "json-schema": "^0.4.0",
    "jsonschema": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "libphonenumber-js": "^1.12.25",
    "long": "^5.2.3",
    "mime": "^4.0.0",
    "mime-types": "^2.1.35",
    "multer": "^2.0.2",
    "nats": "^2.29.1",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "pg": "^8.13.1",
    "pino": "^9.10.0",
    "prisma": "^6.1.0",
    "pusher": "^5.2.0",
    "redis": "^4.7.0",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "tsup": "^8.3.5",
    "undici": "^7.16.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.18",
    "@types/json-schema": "^7.0.15",
    "@types/mime": "^4.0.0",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^24.5.2",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.4.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.7.2",
    "@jest/globals": "^29.7.0",
    "supertest": "^6.3.4"
  }
}
